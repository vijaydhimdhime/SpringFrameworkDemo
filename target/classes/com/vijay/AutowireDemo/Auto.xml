<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        
        <bean id="stud1" class="com.vijay.AutowireDemo.Student" autowire="default">
        	<property name="name" value="vijay"/>
        	<property name="age" value="28"/>
        	<property name="city" value="solapur"/>
        	<!--<property name="pin" ref="pinn"/>  -->
        </bean>
        <bean id="pin" class="com.vijay.AutowireDemo.Pin">
        	<property name="pin" value="410507"/>
        </bean>
        
</beans>