		</property>
	</bean>
	
	<bean name="phone2" class="com.vijay.set.Mobiles">
		<property name="companyName" value="SAMSUNG"/>
		<property name="phoneName">
			<set>
				<value>Galaxy Watch6</value>
				<value>Galaxy F54</value>	
				<value>Galaxy A24 4G</value>
				<value>Galaxy M54</value>
				<value>Galaxy A54</value>		
			</set>
		</property>
	</bean>
	
	
</beans>