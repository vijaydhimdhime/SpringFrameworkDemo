<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->

	<bean class="com.vijay.Addition.Addd" name="add">
		<constructor-arg value="20"/>
		<constructor-arg value="50"/>
	
	</bean>
	<!-- aliase in name attribute name seprated by , ; or white space -->
	<bean class="com.vijay.Addition.Student" name="std1,kunal, kk" scope="prototype">
		<!-- <property name="rollNo" value="101"/>
		<property name="name" value="vijay"/>
		<property name="city" value="pune"/> -->
		<constructor-arg name="rollNo" value="102" index="0"/>
		<constructor-arg name="name" value="sandip" index="1"/>
		<constructor-arg name="city" value="solapur" index="2"/>
		
	
	</bean>
	
</beans>